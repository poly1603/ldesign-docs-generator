{
  "name": "ldesign-docs-example",
  "version": "1.0.0",
  "private": true,
  "description": "LDesign Docs Generator 使用示例",
  "type": "module",
  "scripts": {
    "dev": "ldesign-docs dev",
    "build": "ldesign-docs build",
    "build:spa": "ldesign-docs build --mode spa",
    "build:ssg": "ldesign-docs build --mode ssg",
    "build:hybrid": "ldesign-docs build --mode hybrid",
    "generate": "ldesign-docs generate",
    "preview": "vite preview --outDir dist",
    "clean": "rimraf dist .temp"
  },
  "devDependencies": {
    "@ldesign/docs-generator": "workspace:*",
    "rimraf": "^5.0.0",
    "vite": "^5.0.0"
  }
}
